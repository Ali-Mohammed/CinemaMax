            <script>
                // this script needs to be loaded on every page where an ajax POST may happen
                function getCsrfCookie(name) {
                    function escape(s) {
                        return s.replace(/([.*+?\^${}()|\[\]\/\\])/g, '\\$1');
                    };
                    var match = document.cookie.match(RegExp('(?:^|;\\s*)' + escape(name) + '=([^;]*)'));
                    return match ? match[1] : null;
                }

                csrf_token_name = 'csrf_name';
                csrf_cookie_name = 'csrf_cookie_name';
                $(function ($) {
                    // this bit needs to be loaded on every page where an ajax POST
                    $("input[name=csrf_name]").val(getCsrfCookie(csrf_cookie_name));
                    var object = {};
                    object[csrf_token_name] = getCsrfCookie(csrf_cookie_name);

                    var token = $.cookie("token");

                    $.ajaxSetup({
                        data: object,
                        beforeSend: function (xhr) {
                            if (token) {
                                xhr.setRequestHeader("Authorization", token);
                            }
                        }
                    });
                    $(document).ajaxComplete(function () {

                        object[csrf_token_name] = getCsrfCookie(csrf_cookie_name);
                        $.ajaxSetup({
                            data: object
                        });
                    });
                });

                if ($(window).width() > 760) {
                    $('.dropdown-check').removeAttr('data-toggle');
                }

            </script>

