                document.cookie = "appVideoLocation=" + videoLocation + ";path=/";

                if ((navigator.userAgent.indexOf('Android') != -1)) window.location = "~/deviceApp/android/";
            }

        }


        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }


