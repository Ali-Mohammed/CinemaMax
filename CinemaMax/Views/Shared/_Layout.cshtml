            function initDialog(element) {
                $("#dialog").dialog("open");
                $("#dialog").dialog("widget").position({ my: 'left bottom', at: 'right bottom', of: element });
            }
            function getVideo(videoId) {
                var baseurl = "~/";
                window.location.href = '~/page/movie/watch/en/' + videoId;
            }

            $(document).on('click', '.removeElement', function () {
                var videoId = $(this).attr("data-value");
                var baseurl = "~/";
                $(this).parents('.list_item_container2').remove();
                $.get("~/page/movie/removeNotifications/" + videoId, function (data, status) {
                });
            });
            $("#removeNotifications").click(function () {
                var videoId = '';
                var baseurl = "~/";
                $("#notifications_element").remove();
                $.get("~/page/movie/removeNotifications/" + videoId, function (data, status) {
                });
            });
            $("#noti_Button").one('click', function () {
                load_notifications(0, '');
                setFlag();
            });
            var notifiOffSet = 5;
            $('#notifications').scroll(function () {

                if ($(this).scrollTop() + $(this).height() == $(this)[0].scrollHeight) {
                    load_notifications(notifiOffSet, '');
                    setFlag();
                    notifiOffSet += 5;
                }
            });

            function load_notifications(notifiOffSet, count = '') {
                // call api
                $.get("~/page/movie/getUserSubscriptions/" + notifiOffSet + "/" + count, function (data, status) {
                    var arrTotal = JSON.parse(data);
                    if (typeof arrTotal['new'] !== 'undefined' && arrTotal['new'] !== null) {
                        var arrNew = arrTotal['new'];
                        if (arrNew > 0) {
                            $('#noti_Counter')
                                .css({ opacity: 0 })
                                .text(arrNew).animate({ opacity: 1 }, 500);
                            $('#noti_Counter').css("background", "#E1141E");
                        }
                    }
                    if (typeof arrTotal['notifications'] !== 'undefined' && arrTotal['notifications'] !== null) {
                        var arr = arrTotal['notifications'];
                        for (var i = 0; i < arr.length; i++) {
                            var videoId = arr[i]['nb'];

                            if (arr[i]['web'] == 0 || arr[i]['web'] == 3) {
                                var inner_html = '<div class="list_item_container2">' +
                                    '<i data-value="' + videoId + '"  style="float:right;margin-right:10px" class="fa fa-times  removeElement" aria-hidden="true"></i>' +
                                    '<div class="vertical-text2">New</div>';
                            } else {
                                var inner_html = '<div class="list_item_container2">' +
                                    '<i data-value="' + videoId + '"  style="float:right;margin-right:10px" class="fa fa-times fa-lg removeElement" aria-hidden="true"></i>';
                            }
                            inner_html = inner_html + '<a class="notifi_click" href="#" ' + 'onclick=' + 'getVideo(' + videoId + ')>' +
                                '<div ><img class="image" src="' + arr[i]['img'] + '"  ></div>' +
                                '<div class="label color">' + arr[i]['en_title'] + '</div>' +
                                '<div class="description">Season ' + arr[i]['season'] + ' / Episode ' + arr[i]['episodeNummer'] + ' </div>' +
                                '<div class="vertical-text-light" >' + arr[i]['mDate'] + '</div></a>' +
                                '</div>';

                            $("#notifications_element").append(inner_html);
                            // $("#notifications").getNiceScroll().resize();
                        }
                    }
                });
            }

            function setFlag() {
                $.post("~/page/movie/resetNewNotificationsCount",
                    {});
            }

            // subscriptions
        </script>
