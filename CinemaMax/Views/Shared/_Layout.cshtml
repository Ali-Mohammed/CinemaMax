        if (getCookie('appSeen') == "") {

            if ((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i) || (navigator.userAgent.match(/iPad/i)))) {

                var videoLocation = window.location.href;
                document.cookie = "appVideoLocation=" + videoLocation + ";path=/";

                if (document.cookie.indexOf("iphone_redirect=false") == -1) window.location = "~/deviceApp/iphone/";
            }

            if ((navigator.userAgent.match(/Android/i))) {

